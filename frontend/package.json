{
  "name": "jamalpur-chamber-frontend",
  "version": "1.0.0",
  "description": "Frontend for Jamalpur Chamber of Commerce & Industry",
  "private": true,
  "dependencies": {
    "axios": "^0.27.2",
    "framer-motion": "^7.2.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.8.1",
    "web-vitals": "^2.1.4"
