{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CLIENT_URL": "https://jamalpur-frontend3.vercel.app",
    "FRONTEND_URL": "https://jamalpur-frontend3.vercel.app"
  }
}
